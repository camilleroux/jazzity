= render "musicians/profiles/tabs"

%section#musician-activity
  %h1 Recent Activity
  
  %div.activity
    %ul.activity-list
      - @timeline_events.each do |timeline_event|
        %li
          = image_tag timeline_event.actor.avatar_url, :alt => "#{timeline_event.actor}'s Avatar", :class => "avatar" if timeline_event.actor.has_avatar?
          %cite= timeline_event.actor
          %h3= timeline_event_title(timeline_event)

