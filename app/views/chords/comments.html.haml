= render "index"

%article#chord
  %nav#key-selector
    %h2 Key:
    %ul
      - Key.primaries.each do |key|
        %li{:class => ("current" if @key == key)}= link_to_unless @key == key, key, [key, @chord]

  = render "chords/header"
  = render "chords/tabs"

  = render "comments/index", :commentable => @chord


